<el-dialog\n    ref=\"elDialog\"\n    :visible.sync=\"visible\"\n    :loading=\"loading\"\n    :append-to-body=\"appendToBody\"\n    :width=\"width\"\n    :show-close=\"false\"\n    :destroy-on-close=\"destroyOnClose\"\n    :close-on-click-modal=\"closeOnClickModal\"\n    class=\"small-dialog\"\n    @open=\"open\"\n    @opened=\"opened\"\n    @close=\"close\"\n    @closed=\"closed\"\n  >\n    <el-row slot=\"title\">\n      <el-col :span=\"18\" style=\"text-align: left;\">\n        <common-icon :icon-title=\"dialogTitle || 'Dialog组件'\" :value=\"icon\" style=\"font-size: 1.2em\"/>\n      </el-col>\n      <el-col :span=\"6\" style=\"text-align: right\">\n        <i class=\"el-icon-close\" style=\"font-size: 30px; cursor: pointer\" title=\"关闭\" @click=\"dialogClose\"/>\n      </el-col>\n    </el-row>\n    <div class=\"dialog-body\">\n      <slot/>\n    </div>\n    <slot name=\"footer\">\n      <div class=\"dialog-button\">\n        <el-button v-if=\"buttons.indexOf('cancel') >= 0\" :size=\"size\" :disabled=\"loading\" type=\"info\" title=\"取消\" @click=\"cancel\">取消</el-button>\n        <el-button v-if=\"buttons.indexOf('confirm') >= 0\" :size=\"size\" :disabled=\"loading\" type=\"success\" title=\"确定\" @click=\"confirm\">确定</el-button>\n      </div>\n    </slot>\n  </el-dialog>\n\n</template>\n<script>\n  export default {\n    name: 'SmallDialog',\n    props: {\n      value: { type: Boolean, default: false },\n      dialogTitle: { type: String, default: '' },\n      width: { type: String, default: '50%' },\n      icon: { type: String, default: 'el:el-icon-platform-eleme' },\n      buttons: { type: Array, default: () => ['cancel', 'confirm'] },\n      loading: { type: Boolean, default: false },\n      appendToBody: { type: Boolean, default: false },\n      destroyOnClose: { type: Boolean, default: false },\n      closeOnClickModal: { type: Boolean, default: true }\n    },\n    data() {\n      return {\n        visible: false,\n        size: null\n      };\n    },\n    watch: {\n      value(val) {\n        this.visible = val;\n      },\n      visible(val) {\n        this.$emit('input', val);\n      }\n    },\n    created() {\n    },\n    methods: {\n      open() {\n        this.$emit('open');\n      },\n      opened() {\n        this.$emit('opened');\n      },\n      close() {\n        this.$emit('close');\n      },\n      closed() {\n        this.$emit('closed');\n      },\n      confirm() {\n        this.$emit('confirm');\n      },\n      cancel() {\n        this.$emit('cancel');\n        this.dialogClose();\n      },\n      dialogOpen() {\n      },\n      dialogClose() {\n        this.visible = false;\n      }\n    }\n  };\n</script>\n<style rel=\"stylesheet/scss\" lang=\"scss\">\n  .small-dialog {\n    .el-dialog__header {\n      padding: 5px;\n    }\n    .el-dialog__body {\n      padding: 5px;\n    }\n  }\n</style>\n<style rel=\"stylesheet/scss\" lang=\"scss\" scoped>\n  .small-dialog {\n    .dialog-body {\n      padding-top: 5px;\n    }\n    .dialog-button {\n      margin-top: 10px;\n      text-align: right;\n    }\n  }\n</style>\n\n"]}]}